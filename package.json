{
	"name": "profanitychecker",
	"displayName": "ProfanityChecker",
	"description": "Parse code and check for profane words",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.64.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:profanitychecker.runProfanityChecker",
		"onCommand:extremeprofanitychecker.runExtremeProfanityChecker"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "profanitychecker.runProfanityChecker",
				"title": "Run Profanity Checker"
			},

			{
				"command": "extremeprofanitychecker.runExtremeProfanityChecker",
				"title" : "Run Extreme Profanity Checker"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.64.0",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"@vscode/test-electron": "^2.0.3",
		"eslint": "^8.6.0",
		"glob": "^7.2.0",
		"mocha": "^9.1.3",
		"typescript": "^4.5.4"
	},
	"dependencies": {
		"bad-words": "^3.0.4",
		"badwords-filter": "^1.3.0",
		"badwords-list": "^1.0.0"
	}
}
